<!DOCTYPE html>
<html>
  <head>
    <title>Tree - Login</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <script src="javascripts/jquery-2.1.3.min.js"></script>
    <script src="javascripts/bootstrap.min.js" type="text/javascript"></script>
    <link href="stylesheets/bootstrap.min.css" rel="stylesheet" type="text/css"/>
    <script src="../javascripts/d3.min.js" type="text/javascript"></script>
  </head>
  <body>
    <!-- <h1>GSB Tree</h1> -->
    <script src="../javascripts/logintree.js" type="text/javascript"></script>
    <div class="container">
	    <div id="DIV_1">
			<div id="DIV_2">
				<div id="DIV_3">
					<div id="DIV_4">
						<form action="/" id="FORM_5" name="login" enctype="application/x-www-form-urlencoded" method="post">
							<div id="DIV_6">
								<div id="DIV_7">
									<input type="text" name="username" id="INPUT_8" placeholder="Nom d'utilisateur" />
								</div>
							</div>
							<div id="DIV_9">
								<div id="DIV_10">
									<input type="password" name="password" id="INPUT_11" placeholder="Mot de passe" />
								</div>
							</div>
							<div id="DIV_12">
								<div id="DIV_13">
									<button name="connect" id="BUTTON_14" type="submit">
									</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
		    <div class="col-md-4 col-md-offset-4">
		        <div class="logo-title animated fadeInLeft">
		            <h1 id="login-title">Se connecter</h1>
		        </div>
		    </div>            
		</div>
		<div class="col-md-9 col-centered">
			<% if (message.length > 0) { %>
				<div class="alert alert-info" role="alert"><%= message %></div>
			<% } %>
		</div>
	</div>
	<% include footer.ejs %>
  </body>
</html>
